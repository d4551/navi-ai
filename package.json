                            "cross-env":  "^7.0.3",
                            "electron":  "^38.1.0",
                            "electron-builder":  "^26.0.12",
                            "eslint":  "^9.34.0",
                            "eslint-plugin-import":  "^2.30.0",
                            "eslint-plugin-security":  "^3.0.1",
                            "eslint-plugin-vue":  "^10.4.0",
                            "husky":  "^9.1.6",
                            "jsdom":  "^26.1.0",
                            "lint-staged":  "^15.2.10",
                            "openapi-typescript":  "^7.3.0",
                            "pa11y":  "^9.0.0",
                            "prettier":  "^3.6.2",
                            "process":  "^0.11.10",
                            "sass-embedded":  "^1.91.0",
                            "stylelint":  "^16.24.0",
                            "stylelint-config-standard":  "^39.0.0",
                            "tsx":  "^4.20.5",
                            "typescript":  "^5.6.2",
