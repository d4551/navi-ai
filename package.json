{
  "name": "navi",
  "version": "0.0.1-alpha",
  "description": "NAVI - AI Career Assistant to help gamers transition to professional careers",
  "main": "electron/main.cjs",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "./",
  "license": "LicenseRef-NaviAI-NCPL-1.1",
  "scripts": {
    "dev": "vite",
    "dev:a11y": "vite",
    "dev:host": "vite --host",
    "dev:https": "vite --https",
    "build": "vite build",
    "build:analyze": "vite-bundle-analyzer",
    "build:report": "vite build --mode report",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://127.0.0.1:5173 && electron .\"",
    "electron-dev:port": "concurrently \"VITE_PORT=${VITE_PORT:-5180} vite --port $VITE_PORT --strictPort\" \"wait-on http://127.0.0.1:$VITE_PORT && electron .\"",
    "electron-build": "npm run build && NODE_ENV=production electron-builder",
    "type-check": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "eslint src --ext .js,.vue,.ts --fix",
    "lint:styles": "stylelint \"styles/**/*.css\" \"src/**/*.css\"",
    "lint:check": "eslint src --ext .js,.vue,.ts",
    "lint:security": "eslint src --ext .js,.vue,.ts --config .eslintrc-security.js",
    "lint:fix-all": "node scripts/master-eslint-fix.cjs",
    "lint:fix-props": "node scripts/fix-vue-props.cjs",
    "lint:fix-unused": "node scripts/cleanup-unused-vars.cjs",
    "lint:fix-imports": "node scripts/fix-missing-imports.cjs",
    "lint:clean-duplicates": "node scripts/clean-duplicate-imports.cjs",
    "lint:fix-markdown": "node scripts/fix-markdown-lint.cjs",
    "lint:implement-logic": "node scripts/implement-logic-not-underscores.cjs",
    "format": "prettier --write \"src/**/*.{js,vue,ts,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,ts,css,scss,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "ci:check": "npm run format:check && npm run lint:check && npm run lint:styles && npm run api:types && npm run ci:comments",
    "test:e2e": "npm run build && npm run test:a11y",
    "a11y": "wait-on http://127.0.0.1:$VITE_PORT && node scripts/a11y.cjs",
    "test:a11y": "VITE_PORT=5175 concurrently -k -s first \"vite --port 5175 --strictPort\" \"npm run a11y\"",
    "clean": "rm -rf dist dist-electron node_modules/.vite",
    "clean:all": "rm -rf dist dist-electron node_modules",
    "update-deps": "npm update && npm audit fix",
    "security:audit": "npm audit && npm run lint:security",
    "prepare": "husky || echo 'Husky skipped'",
    "postinstall": "node scripts/postinstall.cjs || echo 'Postinstall completed with warnings'",
    "rebuild:native": "electron-builder install-app-deps",
    "api:types": "openapi-typescript docs/openapi.yaml -o src/shared/types/api.d.ts",
    "api:docs": "redocly build-docs docs/openapi.yaml --output docs/api.html",
    "codemod:mdi:dry": "node scripts/codemods/mdi-to-appicon.cjs --dry",
    "codemod:mdi": "node scripts/codemods/mdi-to-appicon.cjs --apply",
    "codemod:sections:dry": "node scripts/codemods/card-to-section-helpers.cjs --dry",
    "codemod:sections": "node scripts/codemods/card-to-section-helpers.cjs --apply",
    "codemod:sections:views": "node scripts/codemods/card-to-section-helpers.cjs --apply --dir src/views",
    "lint:sections": "node scripts/codemods/card-to-section-helpers.cjs --dry",
    "codemod:mdi-strings": "node scripts/codemods/mdi-string-normalize.cjs --apply",
    "cleanup:comments:dry": "node scripts/cleanup/standardize-comments.cjs --dry",
    "cleanup:comments": "node scripts/cleanup/standardize-comments.cjs --apply",
    "ci:comments": "node scripts/cleanup/check-comments.cjs",
    "migrate:ui": "node scripts/migration-tools/unified-migrator.js",
    "migrate:ui:dry": "node scripts/migration-tools/unified-migrator.js --dry-run --verbose",
    "migrate:tailwind": "node scripts/migration-tools/tailwind-dark-mode-migrator.js",
    "migrate:tailwind:dry": "node scripts/migration-tools/tailwind-dark-mode-migrator.js --dry-run --verbose",
    "migrate:icons": "node scripts/migration-tools/emoji-to-heroicons-migrator.js",
    "migrate:icons:dry": "node scripts/migration-tools/emoji-to-heroicons-migrator.js --dry-run --verbose",
    "migrate:enhanced": "node scripts/migration-tools/enhanced-unified-migrator.js",
    "migrate:enhanced:dry": "node scripts/migration-tools/enhanced-unified-migrator.js --dry-run --verbose",
    "migrate:full": "node scripts/migration-tools/enhanced-unified-migrator.js --verbose",
    "migrate:full:dry": "node scripts/migration-tools/enhanced-unified-migrator.js --dry-run --verbose",
    "unify:system": "node scripts/unified-system-builder.js",
    "unify:system:check": "node scripts/unified-system-builder.js --dry-run",
    "css:build": "tailwindcss -c tailwind.config.js -i ./styles/global.css -o ./public/assets/global.css",
    "css:watch": "tailwindcss -c tailwind.config.js -i ./styles/global.css -o ./public/assets/global.css --watch"
  },
  "build": {
    "appId": "com.navi.app",
    "productName": "NAVI - AI Career Assistant",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.cjs"
    },
    "asar": false,
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@redocly/cli": "2.0.8",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.16",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.6",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "openapi-typescript": "^7.3.0",
    "pa11y": "^9.0.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "sass-embedded": "^1.91.0",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "tsx": "^4.20.5",
    "typescript": "^5.6.2",
    "util": "^0.12.5",
    "vite": "^6.3.6",
    "vite-bundle-analyzer": "^0.11.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.1.6",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.12",
    "@fontsource/electrolize": "^5.2.6",
    "@fontsource/fira-code": "^5.2.6",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/orbitron": "^5.2.6",
    "@fontsource/roboto": "^5.2.6",
    "@google-cloud/text-to-speech": "^6.3.0",
    "@google/genai": "^1.17.0",
    "@google/generative-ai": "^0.24.1",
    "@heroicons/vue": "^2.2.0",
    "@huggingface/inference": "^4.7.1",
    "@mdi/font": "^7.4.47",
    "@mui/icons-material": "^7.3.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sentry/browser": "^10.9.0",
    "@sentry/node": "^10.9.0",
    "@tailwindplus/elements": "^1.0.13",
    "@types/better-sqlite3": "^7.6.13",
    "@vueuse/core": "^11.3.0",
    "@vueuse/integrations": "^11.1.0",
    "ai": "^5.0.33",
    "animejs": "^4.1.3",
    "async-mutex": "^0.5.0",
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "chart.js": "^4.4.1",
    "claude": "^0.1.1",
    "compromise": "^14.14.4",
    "cytoscape": "^3.33.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.1.5",
    "electron-store": "^10.1.0",
    "ethers": "^6.13.2",
    "fast-xml-parser": "^5.2.5",
    "file-saver": "^2.0.5",
    "fuse.js": "^7.1.0",
    "heroicon": "^0.0.2",
    "highlight.js": "^11.9.0",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.0",
    "jsdom": "^23.2.0",
    "jszip": "^3.10.1",
    "keytar": "^7.9.0",
    "kokoro-js": "^1.2.1",
    "lodash-es": "^4.17.21",
    "marked": "^14.1.0",
    "mui": "^1.0.0",
    "natural": "^8.1.0",
    "nprogress": "^0.2.0",
    "ohash": "^1.1.3",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.6.82",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.0.2",
    "qrcode": "^1.5.3",
    "sqlite3": "^5.1.7",
    "validator": "^13.11.0",
    "vue-chartjs": "^5.3.0",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^10.0.1",
    "vue-router": "^4.5.1",
    "vue-toast-notification": "^3.1.3",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.9.6",
    "workbox-window": "^7.1.0",
    "write-excel-file": "^2.3.5",
    "zod": "^3.25.76"
  },
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
